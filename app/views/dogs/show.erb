<h1><%= @dog.name %></h1>
<p>Breed: <%= @dog.breed %></p>
<p>Age: <%= @dog.age %></p>
<h3>Employees: </h3>
<ul>
  <% @dog.employees.each do |employee| %>
    <li><%= link_to employee.full_name, employee %></li>
  <% end %>
</ul>
<%= button_to 'Edit', {action: 'edit', id: @dog.id}, method: :get %>
<%= button_to 'Delete', {action: 'destroy', id: @dog.id}, method: :delete, data: {confirm: 'Are you sure????'} %>
